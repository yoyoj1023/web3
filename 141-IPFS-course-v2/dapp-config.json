{
    "appName": "我的去中心化留言板",
    "version": "1.0.0",
    "description": "基於 IPFS 和智能合約的留言板應用",
    "features": [
      "去中心化儲存",
      "不可篡改記錄",
      "Web3 整合"
    ],
    "settings": {
      "maxMessageLength": 1000,
      "allowAnonymous": true,
      "moderationEnabled": false
    },
    "ipfs": {
      "pinningService": "Pinata",
      "gateway": "https://gateway.pinata.cloud"
    }
  }